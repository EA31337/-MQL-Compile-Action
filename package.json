{
  "name": "MQL-Compile-Action",
  "version": "1.0.0",
  "description": "Compiles MQL programs",
  "main": "lib/main.js",
  "scripts": {
    "build": "ncc build",
    "format": "prettier --write '*.js' dist/index.js && eslint --fix *.js dist/index.js",
    "format-check": "eslint *.js",
    "lint": "eslint *.js",
    "package": "ncc build index.js -o dist",
    "test": "eslint *.js && jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FX31337/MQL-Compile-Action.git"
  },
  "keywords": [
    "github",
    "actions",
    "mql"
  ],
  "author": "kenorb",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FX31337/MQL-Compile-Action/issues"
  },
  "homepage": "https://github.com/FX31337/MQL-Compile-Action/issues",
  "dependencies": {
    "@actions/core": "^1.1",
    "@actions/exec": "^1.0",
    "@actions/github": "^3.0.0",
    "@actions/io": "^1.0",
    "encoding": "^0.1.12",
    "eslint-plugin-jest": "^23.13.2",
    "glob": "^7.1.6",
    "is-docker": "^2.1.1",
    "is-wsl": "^2.2.0",
    "prettier": "^2.0.5"
  },
  "devDependencies": {
    "@zeit/ncc": "^0.20",
    "eslint": "^6.3.0",
    "jest": "^24.9.0",
    "node-stream-zip": "^1.11.2",
    "q": "^1.5.1"
  }
}
